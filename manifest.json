{
  "name": "VideoSkip",
  "short_name": "VideoSkip",
  "description": "__MSG_manifest_descr__",
  "default_locale": "en",
  "version": "0.4",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon24.png",
    "38": "img/icon64.png",
    "48": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "permissions": [
	"activeTab"
  ],
  "web_accessible_resources": [
    "_locales/*",
	"img/icon64.png"
  ],
  "browser_action": {
    "default_icon": "img/icon64.png",
    "default_popup": "popup.html"
  }
}