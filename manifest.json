{
  "name": "VideoSkip",
  "short_name": "VideoSkip",
  "description": "Skips/mutes/blanks selected parts of streaming videos",
  "version": "0.1.5",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon24.png",
    "38": "img/icon64.png",
    "48": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "permissions": [
	"activeTab"
  ],
  "browser_action": {
    "default_icon": "img/icon64.png",
    "default_popup": "popup.html"
  }
}